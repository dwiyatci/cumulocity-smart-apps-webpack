{
  "name": "cumulocity-smart-apps-webpack",
  "version": "3.1.0",
  "description": "A boilerplate for creating Cumulocity Smart Apps instance using webpack-dev-server 3.x",
  "keywords": [
    "cumulocity",
    "m2m",
    "examples",
    "demo",
    "boilerplate",
    "cumulocity smart apps",
    "cumulocity webpack",
    "cumulocity toolkit"
  ],
  "homepage": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack",
  "bugs": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack/issues",
  "license": "MIT",
  "author": "Glenn Dwiyatcita <glenn.dwiyatcita@cumulocity.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "start": "webpack-dev-server",
    "webpack": "yarn clean && webpack",
    "build": "yarn clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf assets",
    "lint": "eslint --cache src"
  },
  "dependencies": {
    "angular-route": "1.5.11",
    "angular-ui-bootstrap": "2.5.6",
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "bootstrap-sass": "3.3.7",
    "cumulocity-clients-javascript": "https://github.com/Cumulocity/cumulocity-clients-javascript"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-loader": "7.1.2",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "bootstrap-loader": "2.2.0",
    "cross-env": "5.1.3",
    "css-loader": "0.28.8",
    "eslint": "4.15.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-loader": "0.5.4",
    "html-webpack-plugin": "2.30.1",
    "imports-loader": "0.7.1",
    "inline-manifest-webpack-plugin": "3.0.1",
    "json-loader": "0.5.7",
    "lodash": "4.17.4",
    "ng-annotate-loader": "0.6.1",
    "node-sass": "4.7.2",
    "resolve-url-loader": "2.2.1",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.2",
    "style-loader": "0.19.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.10.0"
  }
