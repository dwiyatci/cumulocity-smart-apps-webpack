{
  "name": "cumulocity-smart-apps-webpack",
  "version": "3.1.0",
  "description": "A boilerplate for creating Cumulocity Smart Apps instance using webpack-dev-server 3.x",
  "keywords": [
    "cumulocity",
    "m2m",
    "examples",
    "demo",
    "boilerplate",
    "cumulocity smart apps",
    "cumulocity webpack",
    "cumulocity toolkit"
  ],
  "homepage": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack",
  "bugs": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack/issues",
  "license": "MIT",
  "author": "Glenn Dwiyatcita <glenn.dwiyatcita@cumulocity.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/cumulocity-smart-apps-webpack.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "start": "webpack-dev-server",
    "webpack": "yarn clean && webpack",
    "build": "yarn clean && cross-env NODE_ENV=production webpack",
    "clean": "rimraf assets",
    "lint": "eslint --cache src"
  },
  "dependencies": {
    "angular-route": "1.7.0",
    "angular-ui-bootstrap": "2.5.6",
    "babel-polyfill": "6.26.0",
    "babel-runtime": "6.26.0",
    "bootstrap-sass": "3.3.7",
    "cumulocity-clients-javascript": "https://github.com/Cumulocity/cumulocity-clients-javascript"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "bootstrap-loader": "2.2.0",
    "cross-env": "5.1.5",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "json-loader": "0.5.7",
    "lodash": "4.17.10",
    "mini-css-extract-plugin": "0.4.0",
    "ng-annotate-loader": "0.6.1",
    "node-sass": "4.9.0",
    "resolve-url-loader": "2.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "script-loader": "0.7.2",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  }
